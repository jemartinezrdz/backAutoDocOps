{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "AutoDocOps": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=autodocops;Username=postgres;Password=postgres",
    "Redis": "localhost:6379"
  },
  "JwtSettings": {
    "SecretKey": "REPLACE_WITH_SECURE_JWT_SECRET_KEY_32_CHARS_MINIMUM",
    "Issuer": "AutoDocOps",
    "Audience": "AutoDocOps.API",
    "ExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 7
  },
  "OpenAI": {
    "ApiKey": "YOUR_OPENAI_API_KEY",
    "Endpoint": "YOUR_AZURE_OPENAI_ENDPOINT_IF_USING_AZURE",
    "Model": "gpt-3.5-turbo"
  },
  "Stripe": {
    "SecretKey": "YOUR_STRIPE_SECRET_KEY",
    "WebhookSecret": "YOUR_STRIPE_WEBHOOK_SECRET",
    "Plans": {
      "Starter": {
        "PriceId": "YOUR_STARTER_PRICE_ID"
      },
      "Growth": {
        "PriceId": "YOUR_GROWTH_PRICE_ID"
      }
    }
  },
  "Billing": {
    "DefaultSuccessUrl": "http://localhost:8080/success",
    "DefaultCancelUrl": "http://localhost:8080/cancel"
  }
}
